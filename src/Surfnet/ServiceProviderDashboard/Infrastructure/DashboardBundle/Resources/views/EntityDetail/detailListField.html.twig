{% if value is not empty %}
<div class="detail">
    {% if informationPopup is defined %}
        <span title="{{ informationPopup|trans }}" class="help-button">
            <i class="fa fa-question-circle"></i>
        </span>
    {% endif %}
    <label>{{ label }}</label>
    <ul>
    {% for listItem in value %}
        <li>{{ listItem }}</li>
    {% endfor %}
    </ul>
</div>
{% endif %}
